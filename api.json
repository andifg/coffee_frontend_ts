{
  "openapi": "3.0.2",
  "info": {
    "title": "Coffee Backend -  API",
    "version": "0.1.0"
  },
  "paths": {
    "/health": {
      "get": {
        "tags": ["health"],
        "summary": " Get Health",
        "description": "Returns the health status of the coffee service.\n\nReturns:\n    The health report.",
        "operationId": "_get_health_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthStatus"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/coffees/": {
      "post": {
        "tags": ["coffees"],
        "summary": " Post Coffee",
        "operationId": "_post_coffee_api_v1_coffees__post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Coffee"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Coffee"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/coffees": {
      "get": {
        "tags": ["coffees"],
        "summary": " List Coffee",
        "description": "Get list of coffees",
        "operationId": "_list_coffee_api_v1_coffees_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response  List Coffee Api V1 Coffees Get",
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Coffee"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/coffees/ids": {
      "get": {
        "tags": ["coffees"],
        "summary": " List Coffee Ids",
        "description": "Get coffee by id",
        "operationId": "_list_coffee_ids_api_v1_coffees_ids_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response  List Coffee Ids Api V1 Coffees Ids Get",
                  "type": "array",
                  "items": {
                    "type": "string",
                    "format": "uuid"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/coffees/{coffee_id}": {
      "get": {
        "tags": ["coffees"],
        "summary": " Get Coffee By Id",
        "description": "Get coffee by id",
        "operationId": "_get_coffee_by_id_api_v1_coffees__coffee_id__get",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Coffee Id",
              "type": "string",
              "format": "uuid"
            },
            "name": "coffee_id",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Coffee"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["coffees"],
        "summary": " Delete Coffee By Id",
        "description": "Get coffee by id",
        "operationId": "_delete_coffee_by_id_api_v1_coffees__coffee_id__delete",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Coffee Id",
              "type": "string",
              "format": "uuid"
            },
            "name": "coffee_id",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "patch": {
        "tags": ["coffees"],
        "summary": "Endpoint to patch coffee name.",
        "description": "Patch coffee name. To patch ratings use rating endpoints",
        "operationId": "_patch_coffee_api_v1_coffees__coffee_id__patch",
        "parameters": [
          {
            "required": true,
            "schema": {
              "title": "Coffee Id",
              "type": "string",
              "format": "uuid"
            },
            "name": "coffee_id",
            "in": "path"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateCoffee"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Coffee"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Coffee": {
        "title": "Coffee",
        "required": ["_id", "name"],
        "type": "object",
        "properties": {
          "_id": {
            "title": " Id",
            "type": "string",
            "description": "The id of the coffee",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426655440000"
          },
          "name": {
            "title": "Name",
            "type": "string",
            "description": "Name of coffee"
          },
          "ratings": {
            "title": "Ratings",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Rating"
            },
            "description": "Ratings asociated with coffee",
            "default": []
          }
        },
        "description": "Describes a Coffee"
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "HealthStatus": {
        "title": "HealthStatus",
        "required": ["healthy"],
        "type": "object",
        "properties": {
          "healthy": {
            "title": "Healthy",
            "type": "boolean"
          }
        },
        "description": "Health endpoint schema."
      },
      "Rating": {
        "title": "Rating",
        "required": ["_id", "rating"],
        "type": "object",
        "properties": {
          "_id": {
            "title": " Id",
            "type": "string",
            "description": "The id of the rating",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426655440000"
          },
          "rating": {
            "title": "Rating",
            "type": "integer",
            "description": "Ratings for coffee"
          }
        },
        "description": "Describes one rating"
      },
      "UpdateCoffee": {
        "title": "UpdateCoffee",
        "required": ["name"],
        "type": "object",
        "properties": {
          "name": {
            "title": "Name",
            "type": "string",
            "description": "Name of coffee"
          }
        },
        "additionalProperties": false,
        "description": "Describes the update schema for a Coffee"
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": ["loc", "msg", "type"],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  }
}
